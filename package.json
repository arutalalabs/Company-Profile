    "@chromatic-com/storybook": "^4.1.2",
    "@eslint/js": "^9.34.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-docs": "^10.0.6",
